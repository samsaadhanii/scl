},

{
 "chaptno" : "11",
 "slokano" : "018",
 "spart1" : "मांसेऽक्षग्लानिगण्डस्फिक्ऽऽशुष्कतासन्धिवेदनाः।",
"spart2" : "मेदसि क्षीणे कट्याः प्लीह्नो वृद्धिः कृशाङ्गता।।"
},

{
 "chaptno" : "11",
 "slokano" : "019",
 "spart1" : "अस्थ्न्यस्थितोदः शदनं दन्तकेशनखादिषु।",
