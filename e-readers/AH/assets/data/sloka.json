},

{
 "chaptno" : "11",
 "slokano" : "020",
 "spart1" : "शुक्रे चिरात् प्रसिच्येत शुक्रं शोणितमेव वा।",
 "spart2" : "तोदोऽत्यर्थं वृषणयोर्मेढ्रं धूमायतीव च।।"
},

{
 "chaptno" : "11",
 "slokano" : "021",
 "spart1" : "पुरीषे वायुरन्त्राणि सशब्दो वेष्टयन्निव।",
"spart2" : "कुक्षौ भ्रमति यात्यूर्ध्वं हृत्पार्श्वे पीडयन् भृशम्।।"
},

{
 "chaptno" : "11",
 "slokano" : "022",
 "spart1" : "मूत्रेऽल्पं मूत्रयेत्कृच्छ्राद्विवर्णं सास्रमेव वा।",
