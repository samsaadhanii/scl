},

{
 "chaptno" : "11",
 "slokano" : "023",
 "spart1" : "मलानामतिसूक्ष्माणां दुर्लक्ष्यं लक्षयेत् क्षयम्।",
 "spart2" : "स्वमलायनसंशोषतोदशून्यत्वलाघवैः।।"
},

{
 "chaptno" : "11",
 "slokano" : "024",
 "spart1" : "दोषादीनां यथास्वं च विद्याद्दृद्धिक्षयौ भिषक्।",
"spart2" : "क्षयेण विपरीतानां गुणानां वर्धनेन च।।"
},

{
 "chaptno" : "11",
 "slokano" : "025",
 "spart1" : "वृद्धिं मलानां सङ्गाच्च क्षयं चाति विसर्गतः।।",
"spart2" : "मलोचितत्वाद्देहस्य क्षयो वृद्धेस्तु पीडनः।।"
},

{
 "chaptno" : "11",
 "slokano" : "026" ,
 "spart1" : "तत्रास्थनि स्थितो वायुः, पित्तं तु स्वेदरक्तयोः।",
"spart2" : "श्लेष्मा शेषेषु, तेनैषामाश्रयाश्रयिणां मिथः।।"
},

{
 "chaptno" : "11",
 "slokano" : "027",
 "spart1" : "यदेकस्य तदन्यस्य वर्धनक्षपणौषधम्।",
"spart2" : "अस्थिमारुतयोर्नैवं, प्रायो वृद्धिर्हि तर्पणात्।।"
},

{
 "chaptno" : "11",
 "slokano" : "028",
 "spart1" : "श्लेष्मणाऽनुगता तस्मात् सङ्क्षयस्तद्विपर्यात्।",
