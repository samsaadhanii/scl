[
{
 "chaptno" : "01",
 "slokano" : "001",
 "spart1" : "रागादिरोगान् सततानुषक्तानशेषकायप्रसृतानशेषान्।",
 "spart2" : "औत्सुक्यमोहारतिदाञ्जघान योऽपूर्ववैद्याय नमोस्तु तस्मै।।"
},

{
 "chaptno" : "01",
 "slokano" : "001_1",
 "spart1" : "अथात आयुष्कामीयमध्यायं व्याख्यास्यामः।",
 "spart2" : "इति ह स्माहुरात्रेयादयो महर्षयः।"
},

{
 "chaptno" : "01",
 "slokano" : "002",
 "spart1" : "आयुःकामयमानेन धर्मार्थसुखसाधनम्।",
"spart2" : "आयुर्वेदोपदेशेषु विधेयः परमादरः।।"
},

{
 "chaptno" : "01",
 "slokano" : "003",
 "spart1" : "ब्रह्मा स्मृत्वाऽऽयुषो वेदं प्रजापतिमजिग्रहत्।",
"spart2" : "सोऽश्विनौ तौ सहस्राक्षं सोऽत्रिपुत्रादिकान्मुनीन्।।"
},

{
 "chaptno" : "01",
 "slokano" : "004_1",
 "spart1" : "तेऽग्निवेशादिकांस्ते तु पृथक् तन्त्राणि तेनिरे।"
},
{
 "chaptno" : "01",
 "slokano" : "004_2-05_1",
 "spart1" : "तेभ्योऽतिविप्रकीर्णेभ्यः प्रायः सारतरोच्चयः।।",
"spart2" : "क्रियतेऽष्टाङ्गहृदयं नातिसंक्षेपविस्तरम्।"
},

{
 "chaptno" : "01",
 "slokano" : "005_2-06_1",
 "spart1" : "कायबालग्रहोर्ध्वाङ्गशल्यदंष्ट्राजरावृषान्।।",
"spart2" : "अष्टावङ्गानि तस्याहुश्चिकित्सा येषु संश्रिता।"
},

{
 "chaptno" : "01",
 "slokano" : "006_2",
 "spart1" : "वायुः पित्तं कफश्चेति त्रयो दोषाः समासतः।।"
},

{
 "chaptno" : "01",
 "slokano" : "007",
 "spart1" : "विकृताविकृता देहं घ्नन्ति ते वर्तयन्ति च।",
"spart2" : "ते व्यापिनोऽपि हृन्नाभ्योरधोमध्योर्ध्वसंश्रयाः।।"
},

{
 "chaptno" : "01",
 "slokano" : "008",
 "spart1" : "वयोहोरात्रिभुक्तानां तेऽन्तमध्यादिगाः क्रमात्।",
"spart2" : "तैर्भवेद्विषमस्तीक्ष्णो मन्दश्चाग्निः समैः समः।।"
},

{
 "chaptno" : "01",
 "slokano" : "009",
 "spart1" : "कोष्ठः क्रूरो मृदुर्मध्यो मध्यः स्यात्तैः समैरपि।",
"spart2" : "शुक्रार्तवस्थैर्जन्मादौ विषेणेव विषक्रिमेः।।"
},

{
 "chaptno" : "01",
 "slokano" : "010",
 "spart1" : "तैश्च तिस्रः प्रकृतयो हीनमध्योत्तमाः पृथक्।",
"spart2" : "समधातुः समस्तासु श्रेष्ठा निन्द्या द्विदोषजाः।।"
},

{
 "chaptno" : "01",
 "slokano" : "011",
 "spart1" : "तत्र रूक्षो लघुः शीतः खरः सूक्ष्मश्चलोऽनिलः।",
"spart2" : "पित्तं सस्नेहतीक्ष्णोष्णं लघु विस्रं सरं द्रवम्।।"
},

{
 "chaptno" : "01",
 "slokano" : "012",
 "spart1" : "स्निग्धः शीतो गुरुर्मन्दः श्लक्ष्णो मृत्स्नः स्थिरः कफः।",
"spart2" : "संसर्गः सन्निपातश्च तद्द्वित्रिक्षयकोपतः।।"
},

{
 "chaptno" : "01",
 "slokano" : "013",
 "spart1" : "रसासृङ्मांसमेदोऽस्थिमज्जशुक्राणि धातवः।",
"spart2" : "सप्त दूष्याः मला मूत्रशकृत्स्वेदादयोऽपि च।।"
},

{
 "chaptno" : "01",
 "slokano" : "014",
 "spart1" : "वृद्धिः समानैः सर्वेषां विपरीतैर्विपर्ययः।",
"spart2" : "रसाः स्वाद्वम्ललवणतिक्तोषणकषायकाः।।"
},

{
 "chaptno" : "01",
 "slokano" : "015",
 "spart1" : "षड् द्रव्यमाश्रितास्ते च यथापूर्वंबलावहाः।",
"spart2" : "तत्राद्या मारुतं घ्नन्ति त्रयस्तिक्तादयः कफम्।।"
},

{
 "chaptno" : "01",
 "slokano" : "016",
 "spart1" : "कषायतिक्तमधुराः पित्तमन्ये तु कुर्वते।",
"spart2" : "शमनं कोपनं स्वस्थहितं द्रव्यमिति त्रिधा।।"
},

{
 "chaptno" : "01",
 "slokano" : "017",
 "spart1" : "उष्णशीतगुणोत्कर्षात्तत्र वीर्यं द्विधा स्मृतम्।",
"spart2" : "त्रिधा विपाको द्रव्यस्य स्वाद्वम्लकटुकात्मकः।।"
},

{
 "chaptno" : "01",
 "slokano" : "018",
 "spart1" : "गुरुमन्दहिमस्निग्धश्लक्ष्णसान्द्रमृदुस्थिराः।",
"spart2" : "गुणाः ससूक्ष्मविशदा विंशतिः सविपर्ययाः।।"
},

{
 "chaptno" : "01",
 "slokano" : "019",
 "spart1" : "कालार्थकर्मणां योगो हीनमिथ्यातिमात्रकः।",
 "spart2" : "सम्यग्योगश्च विज्ञेयो रोगारोग्यैककारणम्।।"
},

{
 "chaptno" : "01",
 "slokano" : "020",
 "spart1" : "रोगस्तु दोषवैषम्यं, दोषसाम्यमरोगता।",
 "spart2" : "निजागन्तुविभागेन तत्र रोगा द्विधा स्मृताः।।"
},

{
 "chaptno" : "01",
 "slokano" : "021",
 "spart1" : "तेषां कायमनोभेदादधिष्ठानमपि द्विधा।",
"spart2" : "रजस्तमश्च मनसो द्वौ च दोषावुदाहृतौ।।"
},

{
 "chaptno" : "01",
 "slokano" : "022",
 "spart1" : "दर्शनस्पर्शनप्रश्नैः परीक्षेत च रोगिणम्।",
 "spart2" : "रोगं निदानप्राग्रूपलक्षणोपशयाप्तिभिः।।"
},

{
 "chaptno" : "01",
 "slokano" : "023_1",
 "spart1" : "भूमिदेहप्रभेदेन देशमाहुरिह द्विधा।"
},

{
 "chaptno" : "01",
 "slokano" : "023_2-24_1",
 "spart1" : "जाङ्गलं वातभूयिष्ठमनूपं तु कफोल्बणम्।।",
"spart2" : "साधारणं सममलं त्रिधा भूदेशमादिशेत्।"
},

{
 "chaptno" : "01",
 "slokano" : "024_2",
 "spart1" : "क्षणादिर्व्याध्यवस्था च कालो भेषजयोगकृत्।।"
},

{
 "chaptno" : "01",
 "slokano" : "025_1",
 "spart1" : "शोधनं शमनं चेति समासादौषधं द्विधा।"
},

{
 "chaptno" : "01",
 "slokano" : "025_2-26_1",
 "spart1" : "शरीरजानां दोषाणां क्रमेण परमौषधम्।।",
"spart2" : "बस्तिर्विरेको वमनं तथा तैलं घृतं मधु।"
},

{
 "chaptno" : "01",
 "slokano" : "026_2",
 "spart1" : "धीधैर्यात्मादिविज्ञानं मनोदोषौषधं परम्।।"
},

{
 "chaptno" : "01",
 "slokano" : "027",
"spart1" : "भिषक् द्रव्याण्युपस्थाता रोगी पादचतुष्टयम्।",
"spart2" : "चिकित्सितस्य निर्दिष्टं, प्रत्येकं तच्चतुर्गुणम्।।"
},

{
 "chaptno" : "01",
 "slokano" : "028",
 "spart1" : " दक्षस्तीर्थात्तशास्त्रार्थो दृष्टकर्मा शुचिर्भिषक्।",
"spart2" : "बहुकल्पं बहुगुणं सम्पन्नं योग्यमौषधम्।।"
},

{
 "chaptno" : "01",
 "slokano" : "029",
 "spart1" : "अनुरक्तः शुचिर्दक्षो बुद्धिमान् परिचारकः।",
"spart2" : "आढ्यो रोगी भिषग्वश्यो ज्ञापकः सत्त्ववानपि।।"
},

{
 "chaptno" : "01",
 "slokano" : "029.1",
 "spart1" : "साध्योऽसाध्य इति व्याधिर्द्विधा, तौ तु पुनर्द्विधा|",
"spart2" : "सुसाध्यः कृच्छ्रसाध्यश्च, याप्यो यश्चानुपक्रमः।।"
},

{
 "chaptno" : "01",
 "slokano" : "030-31_1",
 "spart1" : "सर्वौषधक्षमे देहे यूनः पुंसो जितात्मनः।अमर्मगोऽल्पहेत्वग्ररूपरूपोऽनुपद्रवः।।",
"spart2" : "अतुल्यदूष्यदेशर्तुप्रकृतिः पादसम्पदि। ग्रहेष्वनुगुणेऽष्वेकदोषमार्गो नवः सुखः।।"
},

{
 "chaptno" : "01",
 "slokano" : "032",
 "spart1" : "शस्त्रादिसाधनः कृच्छ्रः सङ्करे च ततो गदः।",
"spart2" : "शेषत्वादायुषो याप्यः पथ्याभ्यासाद्विपर्यये।।"
},

{
 "chaptno" : "01",
 "slokano" : "033",
"spart1": " अनुपक्रम एव स्यात्स्थितोऽत्यन्तविपर्यये।",
"spart2" : "औत्सुक्यमोहारतिकृद् दृष्टरिष्टोऽक्षनाशनः।।"
},

{
 "chaptno" : "01",
 "slokano" : "034_35",
 "spart1" : "त्यजेदार्तं भिषग्भूपैर्द्विष्टं तेषां द्विषं द्विषम्। हीनोपकरणं व्यग्रमविधेयं गतायुषम्।।",
"spart2" : "चण्डं शोकातुरं भीरुं कृतघ्नं वैद्यमानिनम्। तन्त्रस्यास्य परं चातो वक्ष्यतेऽध्यायसङ्ग्रहः।।"
},
{
 "chaptno" : "02",
 "slokano" : "000",
 "spart1" : "अथातो दिनचर्याध्यायं व्याख्यास्यामः।",
 "spart2" : "इति ह स्माहुरात्रेयादयो महर्षयः।"
},

{
 "chaptno" : "02",
 "slokano" : "001_1",
 "spart1" : "ब्राह्मे मुहूर्त उत्तिष्ठेत्स्वस्थो रक्षार्थमायुषः।"
},

{
 "chaptno" : "02",
 "slokano" : "001_2-003",
 "spart1" : "शरीरचिन्तां निर्वर्त्य कृतशौचविधिस्ततः॥",
 "spart2" : "अर्कन्यग्रोधखदिरकरञ्जककुभादिजम्। प्रातर्भुक्त्वा च मृद्वग्रं कषायकटुतिक्तकम्॥",
 "spart3" : "कनीन्यग्रसमस्थौल्यं प्रगुणं द्वादशाङ्गुलम्। भक्षयेद्दन्तपवनं दन्तमांसान्यबाधयन्॥"
},

{
 "chaptno" : "02",
 "slokano" : "004",
 "spart1" : "नाद्यादजीर्णवमथुश्वासकासज्वरार्दिती।",
"spart2" : "तृष्णास्यपाकहृन्नेत्रशिरःकर्णामयी च तत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "005_1",
 "spart1" : "सौवीरमञ्जनं नित्यं हितमक्ष्णोस्ततो भजेत्।"
},

{
 "chaptno" : "02",
 "slokano" : "005_2-006",
 "spart1" : "चक्षुस्तेजोमयं तस्य विशेषात् श्लेष्मतो भयम्॥",
 "spart2" : "योजयेत्सप्तरात्रेऽस्मात्स्रावणार्थं रसाञ्जनम्।",
 "spart3" : "ततो नावनगण्डूषधूमताम्बूलभाग्भवेत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "007",
 "spart1" : "ताम्बूलं क्षतपित्तास्ररूक्षोत्कुपितचक्षुषाम्।",
"spart2" : "विषमूर्च्छामदार्तानामपथ्यं शोषिणामपि॥"
},

{
 "chaptno" : "02",
 "slokano" : "008",
 "spart1" : "अभ्यङ्गमाचरेन्नित्यं, स जराश्रमवातहा।",
"spart2" : "दृष्टिप्रसादपुष्ट्यायुःस्वप्नसुत्वक्त्वदार्ढ्यकृत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "009",
 "spart1" : "शिरःश्रवणपादेषु तं विशेषेण शीलयेत्।",
"spart2" : "वर्ज्योऽभ्यङ्गः कफग्रस्तकृतसंशुद्ध्यजीर्णिभिः॥"
},

{
 "chaptno" : "02",
 "slokano" : "010",
 "spart1" : "लाघवं कर्मसामर्थ्यं दीप्तोऽग्निर्मेदसः क्षयः।",
"spart2" : "विभक्तघनगात्रत्वं व्यायामादुपजायते॥"
},

{
 "chaptno" : "02",
 "slokano" : "011_1",
 "spart1" : "वातपित्तामयी बालो वृद्धोऽजीर्णो च तं त्यजेत्।"
},

{
 "chaptno" : "02",
 "slokano" : "011_2-012_1",
 "spart1" : "अर्धशक्त्या निषेव्यस्तु बलिभिः स्निग्धभोजिभिः॥",
"spart2" : "शीतकाले वसन्ते च, मन्दमेव ततोऽन्यदा।"
},

{
 "chaptno" : "02",
 "slokano" : "012_2",
 "spart1" : "तं कृत्वाऽनुसुखं देहं मर्दयेच्च समन्ततः॥"
},

{
 "chaptno" : "02",
 "slokano" : "013",
 "spart1" : "तृष्णा क्षयः प्रतमको रक्तपित्तं श्रमः क्लमः।",
"spart2" : "अतिव्यायामतः कासो ज्वरश्छर्दिश्च जायते॥"
},

{
 "chaptno" : "02",
 "slokano" : "014",
 "spart1" : "व्यायामजागराध्वस्त्रीहास्यभाष्यादि साहसम्।",
"spart2" : "गजं सिंह इवाकर्षन् भजन्नति विनश्यति॥"
},

{
 "chaptno" : "02",
 "slokano" : "015",
 "spart1" : "उद्वर्तनं कफहरं मेदसः प्रविलायनम्।",
"spart2" : "स्थिरीकरणमङ्गानां त्वक्प्रसादकरं परम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "016",
 "spart1" : "दीपनं वृष्यमायुष्यं स्नानमूर्जाबलप्रदम्।",
"spart2" : "कण्डूमलश्रमस्वेदतन्द्रातृड्दाहपाप्मजित्॥"
},

{
 "chaptno" : "02",
 "slokano" : "017",
 "spart1" : "उष्णाम्बुनाऽधःकायस्य परिषेको बलावहः।",
"spart2" : "तेनैव तूत्तमाङ्गस्य बलहृत्केशचक्षुषाम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "018",
 "spart1" : "स्नानमर्दितनेत्रास्यकर्णरोगातिसारिषु।",
 "spart2" : "आध्मानपीनसाजीर्णभुक्तवत्सु च गर्हितम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "019",
 "spart1" : "जीर्णो हितं मितं चाद्यान्न वेगानीरयेद्बलात्।",
 "spart2" : "न वेगितोऽन्यकार्यः स्यान्नाजित्वा साध्यमामयम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "020",
 "spart1" : "सुखार्थाः सर्वभूतानां मताः सर्वाः प्रवृत्तयः।",
 "spart2" : "सुखं च न विना धर्मात्तस्माद्धर्मपरो भवेत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "021_1",
 "spart1" : "भक्त्या कल्याणमित्राणि सेवेतेतरदूरगः।"
},

{
 "chaptno" : "02",
 "slokano" : "021_2-022",
 "spart1" : "हिंसास्तेयान्यथाकामं पैशुन्यं परुषानृते॥",
 "spart2" : "सम्भिन्नालापं व्यापादमभिध्यां दृग्विपर्ययम्।",
 "spart3" : "पापं कर्मेति दशधा कायवाङ्मनसैस्त्यजेत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "023",
 "spart1" : "अवृत्तिव्याधिशोकार्ताननुवर्तेत शक्तितः।",
 "spart2" : "आत्मवत्सततं पश्येदपि कीटपिपीलिकम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "024",
 "spart1" : "अर्चयेद्देवगोविप्रवृद्धवैद्यनृपातिथीन्।",
 "spart2" : "विमुखान्नार्थिनः कुर्यान्नावमन्येत नाक्षिपेत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "025",
 "spart1" : "उपकारप्रधानः स्यादपकारपरेऽप्यरौ।",
 "spart2" : "सम्पद्विपत्स्वेकमना, हेतावीर्ष्येत्फले न तु॥"
},

{
 "chaptno" : "02",
 "slokano" : "026",
 "spart1" : "काले हितं मितं ब्रूयादविसंवादि पेशलम्।",
 "spart2" : "पूर्वाभिभाषी, सुमुखः सुशीलः करुणामृदुः॥"
},

{
 "chaptno" : "02",
 "slokano" : "027",
"spart1" : "नैकः सुखी, न सर्वत्र विश्रब्धो, न च शङ्कितः।",
"spart2" : "न कञ्चिदात्मनः शत्रुं नात्मानं कस्यचिद्रिपुम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "028",
 "spart1" : "प्रकाशयेन्नापमानं न च निःस्नेहतां प्रभोः।",
 "spart2" : "जनस्याशयमालक्ष्य यो यथा परितुष्यति॥"
},

{
 "chaptno" : "02",
 "slokano" : "029",
 "spart1" : "तं तथैवानुवर्तेत पराराधनपण्डितः।",
 "spart2" : "न पीडयेदिन्द्रियाणि व चैतान्यति लालयेत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "030",
 "spart1" : "त्रिवर्गशून्यं नारम्भं भजेत्तं चाविरोधयन्|",
 "spart2" : "अनुयायात्प्रतिपदं सर्वधर्मेषु मध्यमाम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "031",
 "spart1" : "नीचरोमनखश्मश्रुर्निर्मलाङ्घ्रिमलायनः।",
 "spart2" : "स्नानशीलः सुसुरभिः सुवेषोऽनुल्बणोज्ज्वलः॥"
},

{
 "chaptno" : "02",
 "slokano" : "032",
 "spart1" : "धारयेत्सततं रत्नसिद्धमन्त्रमहौषधीः।",
 "spart2" : "सातपत्रपदत्राणो विचरेद्युगमात्रदृक्॥"
},

{
 "chaptno" : "02",
 "slokano" : "033_1",
 "spart1": "निशि चात्ययिके कार्ये दण्डी मौली सहायवान्।"
},

{
 "chaptno" : "02",
 "slokano" : "033_2-034_1",
 "spart1" : "चैत्यपूज्यध्वजाशस्तच्छायाभस्मतुषाशुचीन्।",
"spart2" : "नाक्रामेच्छर्करालोष्टबलिस्नानभुवो न च"
},

{
 "chaptno" : "02",
 "slokano" : "034_2",
 "spart1": "नदीं तरेन्न बाहुभ्यां, नाग्निस्कन्धमभिव्रजेत्।"
},

{
 "chaptno" : "02",
 "slokano" : "035",
 "spart1" : "सन्दिग्धनावं वृक्षं च नारोहेद्दुष्टयानवत्।",
 "spart2" : "नासंवृतमुखः कुर्यात्क्षुतिहास्यविजृम्भणम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "036",
 "spart1" : "नासिकां न विकुष्णीयान्नाकस्माद्विलिखेद्भुवम्।",
 "spart2" : "नाङ्गैश्चेष्टेत विगुणं, नासीतोत्कटकश्चिरम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "037_1",
 "spart1" : "देहवाक्चेतसां चेष्टाः प्राक् श्रमाद्विनिवर्तयेत्।",
 "spart2" : "नोर्ध्वजानुश्चिरं तिष्ठेत् ..............॥"
},

{
 "chaptno" : "02",
 "slokano" : "037_1-038",
 "spart1" : "..............नक्तं सेवेत न द्रुमम्॥",
 "spart2" : "तथा चत्वरचैत्यान्तश्चतुष्पथसुरालयान्।",
 "spart3" : "सूनाटवीशून्यगृहश्मशानानि दिवाऽपि न॥"
},

{
 "chaptno" : "02",
 "slokano" : "039",
 "spart1" : "सर्वथेक्षेत नादित्यं, न भारं शिरसा वहेत्।",
 "spart2" : "नेक्षेत प्रततं सूक्ष्मं दीप्तामेध्याप्रियाणि च॥"
},

{
 "chaptno" : "02",
 "slokano" : "040",
 "spart1" : "मद्यविक्रयसन्धानदानादानानि नाचरेत्।",
 "spart2" : "पुरोवातातपरजस्तुषारपरुषानिलान्॥"
},

{
 "chaptno" : "02",
 "slokano" : "041_1",
 "spart1" : "अनृजुः क्षवथूद्गारकासस्वप्नान्नमैथुनम्।"
},

{
 "chaptno" : "02",
 "slokano" : "041_2-042",
 "spart1" :"कूलछायां नृपद्विष्टं व्यालदंष्ट्रिविषाणिनः॥",
  "spart2" :"हीनानार्यातिनिपुणसेवां विग्रहमुत्तमैः।",
 "spart3" : "सन्ध्यास्वभ्यवहारस्त्रीस्वप्नाध्ययनचिन्तनम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "043",
 "spart1" : "शत्रुसत्रगणाकीर्णगणिकापणिकाशनम्।",
 "spart2" : "गात्रवक्त्रनखैर्वाद्यं हस्तकेशावधूननम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "044",
 "spart1" : "तोयाग्निपूज्यमध्येन यानं धूमं शवाश्रयम्।",
 "spart2" : "मद्यातिसक्तिं विश्रम्भस्वातन्त्र्ये स्त्रीषु च त्यजेत्॥"
},

{
 "chaptno" : "02",
 "slokano" : "045",
 "spart1" : "आचार्यः सर्वचेष्टासु लोक एव हि धीमतः।",
 "spart2" : "अनुकुर्यात्तमेवातो लौकिकेऽर्थे परीक्षकः॥"
},

{
 "chaptno" : "02",
 "slokano" : "046",
 "spart1" : "आर्द्रसन्तानता त्यागः कायवाक्चेतसां दमः।",
 "spart2" : "स्वार्थबुद्धिः परार्थेषु पर्याप्तमिति सद्व्रतम्॥"
},

{
 "chaptno" : "02",
 "slokano" : "047",
 "spart1" : "नक्तंदिनानि मे यान्ति कथम्भूतस्य सम्प्रति।",
 "spart2" : "दुःखभाङ्न भवत्येवं नित्यं सन्निहितस्मृतिः॥"
},

{
 "chaptno" : "02",
 "slokano" : "048",
 "spart1" : "इत्याचारः समासेन, यं प्राप्नोति समाचरन्।",
 "spart2" : "आयुरारोग्यमैश्वर्यं यशो लोकांश्च शाश्वतान्॥"
},
{
 "chaptno" : "03",
 "slokano" : "000",
 "spart1" : "अथात ऋतुचर्याध्यायं व्याख्यास्यामः",
 "spart2" : "इति ह स्माहुरात्रेयादयो महर्षयः।।"
},

{
 "chaptno" : "03",
 "slokano" : "001",
 "spart1" : "मासैर्द्विसंख्यैर्माघाद्यैः क्रमात् षडृतवः स्मृताः।",
 "spart2" : "शिशिरोऽथ वसन्तश्च ग्रीष्मो वर्षाशरद्धिमाः।।"
},

{
 "chaptno" : "03",
 "slokano" : "002",
 "spart1" : "शिशिराद्यास्त्रिभिस्तैस्तु विद्यादयनमुत्तरम्।",
"spart2" : "आदानं च, तदादत्ते नृणां प्रतिदिनं बलम्।।"
},

{
 "chaptno" : "03",
 "slokano" : "003",
 "spart1" : "तस्मिन् ह्यत्यर्थतीक्ष्णोष्णरूक्षा मार्गस्वभावतः।",
"spart2" : "आदित्यपवनाः सौम्यान् क्षपयन्ति गुणान् भुवः।।"
},

{
 "chaptno" : "03",
 "slokano" : "004",
 "spart1" : "तिक्तः कषायः कटुको बलिनोऽत्र रसाः क्रमात्।",
 "spart1" : "तस्मादादानमाग्नेयम् ऋतवो दक्षिणायनम्"
},
{
 "chaptno" : "03",
 "slokano" : "005",
 "spart1" : "वर्षादयो विसर्गश्च यद्बलं विसृजत्ययम्।",
"spart2" : "सौम्यत्वादत्र सोमो हि बलवान् हीयते रविः।।"
},

{
 "chaptno" : "03",
 "slokano" : "006",
 "spart1" : "मेघवृष्ट्यनिलैः शीतैः शान्ततापे महीतले।",
"spart2" : "स्निग्धाश्चेहाम्ललवणमधुरा बलिनो रसाः।।"
},

{
 "chaptno" : "03",
 "slokano" : "007",
 "spart1" : "शीतेऽग्र्यं वृष्टिधर्मेऽल्पं बलं मध्यं तु शेषयोः।।",
 "spart1" : "बलिनः शीतसंरोधाद्धेमन्ते प्रबलोऽनलः।।"
},

{
 "chaptno" : "03",
 "slokano" : "008",
 "spart1" : "भवत्यल्पेन्धनो धातून् स पचेद्वायुनेरितः।",
"spart2" : "अतो हिमेऽस्मिन्सेवेत स्वाद्वम्ललवणात्रसान्।।"
},

{
 "chaptno" : "03",
 "slokano" : "009",
 "spart1" : "दैर्घ्यान्निशानामेतर्हि प्रातरेव बुभुक्षितः।",
"spart2" : "अवश्यकार्यं सम्भाव्य यथोक्तं शेएलयेदनु।।"
},

{
 "chaptno" : "03",
 "slokano" : "010",
 "spart1" : "वातघ्नतैलैरभ्यङ्गं मूर्ध्नि तैलं विमर्दनम्।",
"spart2" : "नियुद्धं कुशलैः सार्धं पादाघातं च युक्तितः।।"
},

{
 "chaptno" : "03",
 "slokano" : "011",
 "spart1" : "कषायापहृतस्नेहस्ततः स्नातो यथाविधि।",
"spart2" : "कुङ्कुमेन सदर्पेण प्रदिग्धोऽगुरुधूपितः।।"
},

{
 "chaptno" : "03",
 "slokano" : "012",
 "spart1" : "रसान् स्निग्धान् पलं पृष्टं गौडमच्छसुरां सुराम्।",
"spart2" : "गोधूमपिष्टमाषेक्षुक्षीरोत्थविकृतीः शुभाः।।"
},

{
 "chaptno" : "03",
 "slokano" : "013",
 "spart1" : "नवमन्नं वसां तैलं, शौचकार्ये सुखोदकम्।",
"spart2" : "प्रावाराजिनकौशेयप्रवेणीकौचवास्तृतम्।।"
},

{
 "chaptno" : "03",
 "slokano" : "014",
 "spart1" : "उष्णस्वभावैर्लघुभिः प्रावृतः शयनं भजेत्।",
"spart2" : "युक्त्याऽर्ककिरणान् स्वेदं पादत्राणं च सर्वदा।।"
},

{
 "chaptno" : "03",
 "slokano" : "015",
 "spart1" : "पीवरोरुस्तनश्रोण्यः समदाः प्रमदाः प्रियाः।",
"spart2" : "हरन्ति शीतमुष्णाङ्ग्यो धूपकुङ्कुमयौवनैः।।"
},

{
 "chaptno" : "03",
 "slokano" : "016",
 "spart1" : "अङ्गारतापसन्तप्तगर्भभूवेश्मचारिणः।",
"spart2" : "शीतपारुष्यजनितो न दोषो जातु जायते।।"
},

{
 "chaptno" : "03",
 "slokano" : "017",
 "spart1" : "अयमेव विधिः कार्यः शिशिरेऽपि विशेषतः।",
"spart2" : "तदा हि शेएतमधिकं रौक्ष्यं चादानकालजम्।।"
},

{
 "chaptno" : "03",
 "slokano" : "018",
 "spart1" : "कफश्चितो हि शिशिरे वसन्तेऽर्कांशुतापितः।",
"spart2" : "हत्वाऽग्निं कुरुते रोगानतस्तं त्वरया जयेत्।।"
},

{
 "chaptno" : "03",
 "slokano" : "019",
 "spart1" : "तीक्ष्णैर्वमननस्याद्यैर्लघुरूक्षैश्च भोजनैः।",
"spart2" : "व्यायामोद्वर्तनाघातैर्जित्वा श्लेष्माणमुल्बणम्।।"
},

{
 "chaptno" : "03",
 "slokano" : "020",
 "spart1" : "स्नातोऽनुलिप्तः कर्पूरचन्दनागुरुकुङ्कुमैः।",
 "spart2" : "पुराणयवगोधूमक्षौद्रजाङ्गलशूल्यभुक्।।"
},

{
 "chaptno" : "03",
 "slokano" : "021",
 "spart1" : "सहकाररसोन्मिश्रानास्वाद्य प्रिययाऽर्पितान्।",
 "spart2" : "प्रियास्यसङ्गसुरभीन् प्रियानेत्रोत्पलाङ्कितान्।।"
},

{
 "chaptno" : "03",
 "slokano" : "022",
 "spart1" : "सौमनस्यकृतो हृद्यान्वयस्यैः सहितः पिबेत्।",
"spart2" : "निर्गदानासवारिष्टसीधुमार्द्वीकमाधवान्।।"
},

{
 "chaptno" : "03",
 "slokano" : "023",
 "spart1" : "शृङ्गबेराम्बु साराम्बु मध्वम्बु जलदाम्बु च।",
 "spart2" : "दक्षिणानिलशीतेषु परितो जलवाहिषु।।"
},

{
 "chaptno" : "03",
 "slokano" : "024",
 "spart1" : "अदृष्टनष्टसूर्येषु मणिकुट्टिमकान्तिषु।",
 "spart1" : "परपुष्टविघुष्टेषु कामकर्मान्तभूमिषु।।"
},

{
 "chaptno" : "03",
 "slokano" : "025",
 "spart1" : "विचित्रपुष्पवृक्षेषु काननेषु सुगन्धिषु।।",
"spart2" : "गोष्ठीकथाभिश्चित्राभिर्मध्याह्नं गमयेत्सुखी।"
},

{
 "chaptno" : "03",
 "slokano" : "026",
 "spart1" : "गुरुशीतदिवास्वप्नस्निग्धाम्लमधुरांस्त्यजेत्",
 "spart1" : "तीक्ष्णांशुरतितीक्ष्णांशुर्ग्रीष्मे संक्षिपतीव यत्।।"
},

{
  "chaptno" : "03",
  "slokano" : "027",
  "spart1" : "प्रत्यहं क्षीयते श्लेष्मा तेन वायुश्च वर्धते।",
  "spart1" : "अतोऽस्मिन्पटुकट्वम्लव्यायामार्ककरांस्त्यजेत्|।।"
},

{
 "chaptno" : "03",
 "slokano" : "028",
 "spart1" : "भजेन्मधुरमेवान्नं लघु स्निग्धं हिमं द्रवम्।",
"spart2" : "सुशीततोयसिक्ताङ्गो लिह्यात्सक्तून् सशर्करान्।।"
},

{
 "chaptno" : "03",
 "slokano" : "029",
 "spart1" : "मद्यं न पेयं, पेयं वा स्वल्पं, सुबहुवारि वा।",
 "spart1" : "अन्यथाशोषशैथिल्यदाहमोहान् करोति तत्।।"
},

{
 "chaptno" : "03",
 "slokano" : "030",
"spart1" : "कुन्देन्दुधवलं शालिमश्नीयाज्जाङ्गलैः पलैः।",
"spart2" : "पिबेद्रसं नातिघनं रसालां रागखाण्डवौ।।"
},

{
 "chaptno" : "03",
 "slokano" : "031",
 "spart1" : "पानकं पञ्चसारं वा नवमृद्भाजने स्थितम्।",
"spart2" : "मोचचोचदलैर्युक्तं साम्लं मृन्मयशुक्तिभिः।।"
},

{
 "chaptno" : "03",
 "slokano" : "032",
 "spart1" : "पाटलावासितं चाम्भः सकर्पूरं सुशीतलम्।",
"spart2" : "शशाङ्ककिरणान् भक्ष्यान् रजन्यां भक्षयन् पिबेत्।।"
},

{
 "chaptno" : "03",
 "slokano" : "033",
 "spart1" : "ससितं माहिषं क्षीरं चन्द्रनक्षत्रशीतलम्।",
"spart2" : "अभ्रङ्कषमहाशालतालरुद्धोष्णरश्मिषु।।"
},

{
 "chaptno" : "03",
 "slokano" : "034",
 "spart1" : "वनेषु माधवीश्लिष्टद्राक्षास्तबकशालिषु।",
"spart2" : "सुगन्धिहिमपानीयसिच्यमानपटालिके।।"
},

{
 "chaptno" : "03",
 "slokano" : "035",
 "spart1" : "कायमाने चिते चूतप्रवालफललुम्बिभिः।",
"spart2" : "कदलीदलकह्लारमृणालकमलोत्पलैः।।"
},

{
"chaptno" : "03",
"slokano" : "036",
"spart1": "कोमलैः कल्पिते तल्पे हसत्कुसुमपल्लवे।",
"spart2" : "मध्यंदिनेऽर्कतापार्तः स्वप्याद्धारागृहेऽथवा।।"
},

{
 "chaptno" : "03",
 "slokano" : "037",
 "spart1" : "पुस्तस्त्रीस्तनहस्तास्यप्रवृत्तोशीरवारिणि।",
 "spart2" : "निशाकरकराकीर्णे सौधपृष्ठे निशासु च।।"
},

{
    "chaptno" : "03",
    "slokano" : "038",
    "spart1" : "आसना स्वस्थचित्तस्य चन्दनार्द्रस्य मालिनः।",
    "spart2" : "निवृत्तकामतन्त्रस्य सुसूक्ष्मतनुवाससः।।"
},

{
    "chaptno" : "03",
    "slokano" : "039",
    "spart1" : "जलार्द्रास्तालवृन्तानि विस्तृताः पद्मिनीपुटाः।",
    "spart2" : "उत्क्षेपाश्च मृदूत्क्षेपा जलवर्षिहिमानिलाः।।"
},

{
    "chaptno" : "03",
    "slokano" : "040",
    "spart1" : "कर्पूरमल्लिकामाला हाराः सहरिचन्दनाः।",
    "spart2" : "मनोहरकलालापाः शिशवः सारिकाः शुकाः।।"
},

{
    "chaptno" : "03",
    "slokano" : "041",
    "spart1" : "मृणालवलयाः कान्ताः प्रोत्फुल्लकमलोज्ज्वलाः।",
    "spart2" : "उजङ्गमा इव पद्मिन्यो हरन्ति दयिताः क्लमम्।।"
},

{
    "chaptno" : "03",
    "slokano" : "042",
    "spart1" : "आदानग्लानवपुषामग्निः सन्नोऽपि सीदति।",
    "spart2" : "वर्षासु दोषैर्दुष्यन्ति तेऽम्बुलम्बाम्बुदेऽम्बरे।।"
},

{
    "chaptno" : "03",
    "slokano" : "043",
    "spart1" : "सतुषारेण मरुता सहसा शीतलेन च।",
    "spart2" : "भूबाष्पेणाम्लपाकेन मलिनेन च वारिणा।।"
},

{
    "chaptno" : "03",
    "slokano" : "044",
    "spart1" : "वह्निनैव च मन्देन, तेष्वित्यन्योन्यदूषिषु।",
    "spart2" : "भजेत्साधारणं सर्वमूष्मणस्तेजनं च यत्।।"
},

{
    "chaptno" : "03",
    "slokano" : "045",
    "spart1" : "आस्थापनं शुद्धतनुर्जीर्णं धान्यं रसान् कृतान्।",
    "spart2" : "जाङ्गलं पिशितं यूषान् मध्वरिष्टं चिरन्तनम्।।"
},

{
    "chaptno" : "03",
    "slokano" : "046",
    "spart1" : "मस्तु सौवर्चलाढ्यं वा पञ्चकोलावचूर्णितम्।",
    "spart2" : "दिव्यं कौपं शृतं चाम्भो भोजनं त्वतिदुर्दिने।।"
},

{
    "chaptno" : "03",
    "slokano" : "047",
    "spart1" : "व्यक्ताम्ललवणस्नेहं संशुष्कं क्षौद्रवल्लघु।",
    "spart2" : "अपादचारी सुरभिः सततं धूपिताम्बरः।।"
},

{
    "chaptno" : "03",
    "slokano" : "048",
    "spart1" : "हर्म्यपृष्ठे वसेद्बाष्पशीतशीकरवर्जिते।",
    "spart2" : "नदीजलोदमन्थाहःस्वप्नायासातपांस्त्यजेत्।।"
},

{
    "chaptno" : "03",
    "slokano" : "049",
    "spart1" : "वर्षाशीतोचिताङ्गानां सहसैवार्करश्मिभिः।",
    "spart2" : "तप्तानां सञ्चितं वृष्टौ पित्तं शरदि कुप्यति।।"
},

{
    "chaptno" : "03",
    "slokano" : "050",
    "spart1" : "तज्जयाय घृतं तिक्तं विरेको रक्तमोक्षणम्।",
    "spart2" : "तिक्तं स्वादु कषायं च क्षुधितोऽन्नं भजेल्लघु।।"
},

{
    "chaptno" : "03",
    "slokano" : "051",
    "spart1" : "शालिमुद्गसिताधात्रीपटोलमधुजाङ्गलम्।",
    "spart2" : "तप्तं तप्तांशुकिरणैः शीतं शीतांशुरश्मिभिः।।"
},

{
    "chaptno" : "03",
    "slokano" : "052",
    "spart1" : "समन्तादप्यहोरात्रमगस्त्योदयनिर्विषम्।",
    "spart2" : "शुचि हंसोदकं नाम निर्मलं मलजिज्जलम्।।"
},

{
    "chaptno" : "03",
    "slokano" : "053",
    "spart1" : "नाभिष्यन्ति न वा रूक्षं पानादिष्वमृतोपमम्।",
    "spart2" : "चन्दनोशीरकर्पूरमुक्तास्रग्वसनोज्ज्वलः।।"
},

{
    "chaptno" : "03",
    "slokano" : "054",
    "spart1" : "सौधेषु सौधधवलां चन्दिकां रजनीमुखे।",
    "spart2" : "तुषारक्षारसौहित्यदधितैलवसातपान्।।"
},

{
    "chaptno" : "03",
    "slokano" : "055",
    "spart1" : "तीक्ष्णमद्यदिवास्वप्नपुरोवातान् परित्यजेत्।",
    "spart2" : "शीते वर्षासु चाद्यांस्त्रीन् वसन्तेऽन्त्यान् रसान्भजेत्।।"
},

{
    "chaptno" : "03",
    "slokano" : "056",
    "spart1" : "स्वादुं निदाघे, शरदि स्वादुतिक्तकषायकान्।",
    "spart2" : "शरद्वसन्तयो रूक्षं शीतं घर्मघनान्तयोः।।"
},

{
    "chaptno" : "03",
    "slokano" : "057",
    "spart1" : "अन्नपानं समासेन विपरीतमतोऽन्यदा।",
    "spart2" : "नित्यं सर्वरसाभ्यासः स्वस्वाधिक्यमृतावृतौ।।"
},

{
    "chaptno" : "03",
    "slokano" : "058",
    "spart1" : "ऋत्वोरन्त्यादिसप्ताहावृतुसन्धिरिति स्मृतः।",
    "spart2" : "तत्र पूर्वो विधिस्त्याज्यः सेवनीयोऽपरः क्रमात्।।"
},

{
    "chaptno" : "03",
    "slokano" : "059",
    "spart1" : "असात्म्यजा हि रोगाः स्युः सहसा त्यागशीलनात्।"
},

{
 "chaptno" : "04",
 "slokano" : "000",
 "spart1" : "अथातो रोगानुत्पादनीयाध्यायं व्याख्यास्यामः।",
 "spart2" : "इति ह स्माहुरात्रेयादयो महर्षयः।"
},

{
 "chaptno" : "04",
 "slokano" : "001",
 "spart1" : "वेगान्न धारयेद्वातविण्मूत्रक्षवतृट्क्षुधाम्।",
 "spart2" : "निद्राकासश्रमश्वासजृम्भाश्रुच्छर्दिरेतसाम्॥"
},

{
 "chaptno" : "04",
 "slokano" : "002",
 "spart1" : "अधोवातस्य रोधेन गुल्मोदावर्तरुक्क्लमाः।",
 "spart2" : "वातमूत्रशकृत्सङ्गदृष्ट्यग्निवधहृद्गदाः॥"
},

{
 "chaptno" : "04",
 "slokano" : "003-004_1",
 "spart1" : "शकृतः पिण्डिकोद्वेष्टप्रतिश्यायशिरोरुजः।",
 "spart2" : "ऊर्ध्ववायुः परीकर्तो हृदयस्योपरोधनम्॥",
 "spart3" : "मुखेन विट्प्रवृत्तिश्च पूर्वोक्ताश्चामयाः स्मृताः।"
},

{
 "chaptno" : "04",
 "slokano" : "004_2-005",
 "spart1" : "अङ्गभङ्गाश्मरीबस्तिमेढ्रवंक्षणवेदनाः॥",
 "spart2" : "मूत्रस्य रोधात्पूर्वे च प्रायो रोगाः तदौषधम्।",
 "spart3" : "वर्त्यभ्यङ्गावगाहाश्च स्वेदनं बस्तिकर्म च॥"
},

{
 "chaptno" : "04",
 "slokano" : "006_1",
 "spart1" : "अन्नपानं च विड्भेदि विड्रोधोत्थेषु यक्ष्मसु।"
},

{
 "chaptno" : "04",
 "slokano" : "006_2-007_1",
 "spart1" : "मूत्रजेषु तु पाने च प्राग्भक्तं शस्यते घृतम्॥",
 "spart2" : "जीर्णान्तिकं चोत्तमया मात्रया योजनाद्वयम्।",
 "spart3" : "अवपीडकमेतच्च संज्ञितं .........॥"
},

{
 "chaptno" : "04",
 "slokano" : "007_2-008",
 "spart1" : "............... धारणात्पुनः॥",
 "spart2" : "उद्गारस्यारुचिः कम्पो विबन्धो हृदयोरसोः।",
 "spart3" : "आध्मानकासहिध्माश्च हिध्मावत्तत्र भेषजम्॥"
},

{
 "chaptno" : "04",
 "slokano" : "009_1",
 "spart1" : "शिरोर्तीन्द्रियदौर्बल्यमन्यास्तम्भार्दितं क्षुतेः।"
},

{
 "chaptno" : "04",
 "slokano" : "009_2-010_1",
 "spart1" : "तीक्ष्णधूमाञ्जनाघ्राणनावनार्कविलोकनैः॥",
 "spart2" : "प्रवर्तयत्क्षुतिं सक्तां स्नेहस्वेदौ च शीलयेत्।"
},

{
 "chaptno" : "04",
 "slokano" : "010_2-011_1",
 "spart1" : "शोषाङ्गसादबाधिर्यसम्मोहभ्रमहृद्गदाः॥",
 "spart2" : "तृष्णाया निग्रहात्तत्र शीतः सर्वो विधिर्हितः।"
},

{
 "chaptno" : "04",
 "slokano" : "011_2-012_1",
 "spart1" : "अङ्गभङ्गारुचिग्लानिकार्श्यशूलभ्रमाः क्षुधः॥",
 "spart2" : "तत्र योज्यं लघु स्निग्धमुष्णमल्पं च भोजनम्।"
},

{
 "chaptno" : "04",
 "slokano" : "012_2-013_1",
 "spart1" : "निद्राया मोहमूर्धाक्षिगौरवालस्यजृम्भिकाः॥",
 "spart2" : "आङ्गमर्दश्च, तत्रेष्टः स्वप्नः संवाहनानि च।"
},

{
 "chaptno" : "04",
 "slokano" : "013_2-014_1",
 "spart1" : "कासस्य रोधात्तद्वृद्धिः श्वासारुचिहृदामयाः॥",
 "spart2" : "शोषे हिध्मा च, कार्योऽत्र कासहा सुतरां विधिः।"
},

{
 "chaptno" : "04",
 "slokano" : "014_2-015_1",
 "spart1" : "गुल्महृद्रोगसम्मोहाः श्रमश्वासाद्विधारितात्॥",
 "spart2" : "हितं विश्रमणं तत्र वातघ्नश्च क्रियाक्रमः।"
},

{
 "chaptno" : "04",
 "slokano" : "015_2",
 "spart1" : "जृम्भायाः क्षववद्रोगाः, सर्वश्चानिलजिद्विधिः॥"
},

{
 "chaptno" : "04",
 "slokano" : "016",
 "spart1" : "पीनसाक्षिशिरोहृद्रुङ्मन्यास्तम्भारुचिभ्रमाः।",
"spart2" : "सगुल्मा बाष्पतस्तत्र स्वप्नो मद्यं प्रियाः कथाः॥"
},

{
 "chaptno" : "04",
 "slokano" : "017",
 "spart1" : "विसर्पकोठकुष्ठाक्षिकण्डूपाण्ड्वामयज्वराः।",
 "spart2" : "सकासश्वासहृल्लासव्यङ्गश्वयथवो वमेः॥"
},

{
 "chaptno" : "04",
 "slokano" : "018",
 "spart1" : "गण्डूषधूमानाहारा रूक्षं भुक्त्वा तदुद्वमः।",
 "spart2" : "व्यायामः स्रुतिरस्रस्य शस्तं चात्र विरेचनम्॥"
},

{
 "chaptno" : "04",
 "slokano" : "019_1",
 "spart1" : "सक्षारलवणं तैलमभ्यङ्गार्थं च शस्यते।"
},

{
 "chaptno" : "04",
 "slokano" : "019_2-020_1",
 "spart1" : "शुक्रात्तत्स्रवणं गुह्यवेदनाश्वयथुज्वराः॥",
 "spart2" : "हृद्व्यथामूत्रसङ्गाङ्गभङ्गवृद्ध्यश्मषण्ढताः।"
},

{
 "chaptno" : "04",
 "slokano" : "020_2-021_1",
 "spart1" : "ताम्रचूडसुराशालिबस्त्यभ्यङ्गावगाहनम्॥",
 "spart2" : "बस्तिशुद्धिकरैः सिद्धम् भजेत्क्षीरं प्रियाः स्त्रियः।"
},

{
 "chaptno" : "04",
 "slokano" : "021_2",
 "spart1" : "तृट्शूलार्तं त्यजेत् क्षीणं विड्वमं वेगरोधिनम्॥"
},

{
 "chaptno" : "04",
 "slokano" : "022",
 "spart1" : "रोगाः सर्वेऽपि जायन्ते वेगोदीरणधारणैः।",
 "spart2" : "निर्दिष्टं साधनं तत्र भूयिष्ठं ये तु तान् प्रति॥"
},


{
 "chaptno" : "04",
 "slokano" : "023",
 "spart1" : "ततश्चानेकधा प्रायः पवनो यत्प्रकुप्यति।",
 "spart2" : "अन्नपानौषधं तस्य युञ्जीतातोऽनुलोमनम्॥"
},

{
 "chaptno" : "04",
 "slokano" : "024",
 "spart1" : "धारयेत्तु सदा वेगान् हितैषी प्रेत्य चेह च।",
 "spart2" : "लोभेर्ष्याद्वेषमात्सर्यरागादीनां जितेन्द्रियः॥"
},

{
 "chaptno" : "04",
 "slokano" : "025",
 "spart1" : "यतेत च यथाकालं मलानां शोधनं प्रति।",
 "spart2" : "अत्यर्थसञ्चितास्ते हि ऋद्धाः स्युर्जीवितच्छिदः॥"
},

{
 "chaptno" : "04",
 "slokano" : "026",
 "spart1" : "दोषाः कदाचित्कुप्यन्ति जिता लङ्घनपाचनैः।",
 "spart2" : "ये तु संशोधनैः शुद्धा न तेषां पुनरुद्भवः॥"
},

{
 "chaptno" : "04",
 "slokano" : "027",
 "spart1" : "यथाक्रमं यथायोगमत ऊर्ध्वं प्रयोजयेत्।",
 "spart2" : "रसायनानि सिद्धानि वृष्ययोगांश्च कालवित्॥"
},

{
 "chaptno" : "04",
 "slokano" : "028-029",
 "spart1" : "भेषजक्षपिते पथ्यमाहारैर्बृंहणं क्रमात्। शालिषष्टिकगोधूममुद्गमांसघृतादिभिः॥",
 "spart2" : "हृद्यदीपनभैषज्यसंयोगाद्रुचिपक्तिदैः। साभ्यङ्गोद्वर्तनस्नाननिरूहस्नेह बस्तिभिः॥"
},

{
 "chaptno" : "04",
 "slokano" : "030",
 "spart1" : "तथा स लभते शर्म सर्वपावकपाटवम्।",
 "spart2" : "धीवर्णेन्द्रियवैमल्यं वृषतां दैर्घ्यमायुषः॥"
},

{
 "chaptno" : "04",
 "slokano" : "031",
 "spart1" : "ये भूतविषवाय्वग्निक्षतभङ्गादिसम्भवाः।",
 "spart2" : "रागद्वेषभयाद्याश्च ते स्युरागन्तवो गदाः॥"
},

{
 "chaptno" : "04",
 "slokano" : "032-034",
 "spart1" : "त्यागः प्रज्ञापराधानामिन्द्रियोपशमः स्मृतिः। देशकालात्मविज्ञानं सद्वृत्तस्यानुवर्तनम्॥",
 "spart2" : "अथर्वविहिता शान्तिः प्रतिकूलग्रहार्चनम्। भूताद्यस्पर्शनोपायो निर्दिष्टश्च पृथक् पृथक्॥",
 "spart3" : "अनुत्पत्त्यै समासेन विधिरेषः प्रदर्शितः। निजागन्तुविकाराणामुत्पन्नानां च शान्तये॥"
},

{
 "chaptno" : "04",
 "slokano" : "035",
 "spart1" : "शीतोद्भवं दोषचयं वसन्ते विशोधनं ग्रीष्मजमभ्रकाले|।",
 "spart2" : "घनात्यये वार्षिकमाशु सम्यक् प्राप्नोति रोगानृतुजान्न जातु॥"
},

{
 "chaptno" : "04",
 "slokano" : "036",
 "spart1" : "नित्यं हिताहारविहारसेवी समीक्ष्यकारी विषयेष्वसक्तः।",
 "spart2" : "दाता समः सत्यपरः क्षमावानाप्तोपसेवी च भवत्यरोगः॥"
},

{
 "chaptno" : "11",
 "slokano" : "000",
 "spart1" : "अथातो दोषादिविज्ञानीयमध्यायं व्याख्यास्यामः।",
 "spart2" : "इति ह स्माहुरात्रेयादयो महर्षयः।"
},

{
 "chaptno" : "11",
 "slokano" : "001-002_1",
 "spart1" : "दोषधातुमला मूलं सदा देहस्य तं चलः। उत्साहोच्छ्वासनिश्वासचेष्टावेगप्रवर्तनैः।। ",
 "spart2" : "सम्यग्गत्या च धातूनामक्षाणां पाटवेन च|अनुगृह्णात्यविकृतः,...........।।"
},

{
 "chaptno" : "11",
 "slokano" : "002_2-003_1",
 "spart1" : "..............पित्तं पक्त्यूष्मदर्शनैः।।",
"spart2" : "क्षुत्तृड्रुचिप्रभामेधाधीशौर्यतनुमार्दवैः|"
},

{
 "chaptno" : "11",
 "slokano" : "003_2",
 "spart1" : "श्लेष्मा स्थिरत्वस्निग्धत्वसन्धिबन्धक्षमादिभिः।।"
},

{
 "chaptno" : "11",
 "slokano" : "004",
 "spart1" : "प्रीणनं जीवनं लेपः स्नेहो धारणपूरणे।",
 "spart2" : "गर्भोत्पादश्च धातूनां श्रेष्ठं कर्म क्रमात्स्मृतम्।।"
},
{
 "chaptno" : "11",
 "slokano" : "005_1",
 "spart1" : "अवष्टम्भः पुरीषस्य, मूत्रस्य क्लेदवाहनम्।",
 "spart2" : "स्वेदस्य क्लेदविधृतिः.............।।"
},

{
 "chaptno" : "11",
 "slokano" : "005_2-006",
 "spart1" : "...............वृद्धस्तु कुरुतेऽनिलः।।",
"spart2" : "कार्श्यकार्ष्ण्योष्णकामत्वकम्पानाहशकृद्ग्रहान्। बलनिद्रेन्द्रियभ्रंशप्रलापभ्रमदीनताः।।"
},

{
 "chaptno" : "11",
 "slokano" : "007_1",
 "spart1" : "पीतविण्मूत्रनेत्रत्वक्क्षुत्तृड्दाहाल्पनिद्रताः।",
 "spart2" : "पित्तम्......................।।"
},

{
 "chaptno" : "11",
 "slokano" : "007_2-008_1",
 "spart1" : ".....श्लेष्माऽग्निसदनप्रसेकालस्यगौरवम्।",
"spart2" : "श्वैत्यशैत्यश्लथाङ्गत्वं श्वासकासातिनिद्रताः।।"
},

{
 "chaptno" : "11",
 "slokano" : "008_2-009",
 "spart1" : "रसोऽपि श्लेष्मवत् रक्तं विसर्पप्लीहविद्रधीन्।।",
"spart2" : "कुष्ठवातास्रपित्तास्रगुल्मोपकुशकामलाः। व्यङ्गाग्निनाशसम्मोहरक्तत्वङ्नेत्रमूत्रताः।।"
},

{
 "chaptno" : "11",
 "slokano" : "010",
 "spart1" : "मांसं गण्डार्बुदग्रन्थिगण्डोरूदरवृद्धिताः।",
"spart2" : "कण्ठादिष्वधिमांसं च तद्वन्मेदस्तथा श्रमम्।।"
},

{
 "chaptno" : "11",
 "slokano" : "011_1",
 "spart1" :"अल्पेऽपि चेष्टिते श्वासं स्फिक्स्तनोदरलम्बनम्।"
},

{
 "chaptno" : "11",
 "slokano" : "011_2-012_1",
 "spart1" : "अस्थ्यध्यस्थ्यधिदन्तांश्च मज्जा नेत्राङ्गगौरवम्।।",
"spart2" : "पर्वसु स्थूलमूलानि कुर्यात्कृच्छ्राण्यरूंषि च।"
},

{
 "chaptno" : "11",
 "slokano" : "012_2",
 "spart1" : "अतिस्त्रीकामतां वृद्धं शुक्रं शुक्राश्मरीमपि।।"
},

{
 "chaptno" : "11",
 "slokano" : "013",
 "spart1" : "कुक्षावाध्मानमाटोपं गौरवं वेदनां शकृत्।",
"spart2" : "मूत्रं तु बस्तिनिस्तोदं कृतेऽप्यकृतसंज्ञताम्।।"
},

{
 "chaptno" : "11",
 "slokano" : "014",
 "spart1" : "स्वेदोऽतिस्वेददौर्गन्ध्यकण्डूः एवं च लक्षयेत्।",
"spart2" : "दूषिकादीनपि मलान् बाहुल्यगुरुतादिभिः।।"
},

{
 "chaptno" : "11",
 "slokano" : "015",
 "spart1" : "लिङ्गं क्षीणेऽनिलेऽङ्गस्य सादोऽल्पं भाषितेहितम्।",
