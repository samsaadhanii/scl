},

{
 "chaptno" : "11",
 "slokano" : "038-039_1",
 "spart1" : "स्निग्धं सोमात्मकं शुद्धमीषल्लोहितपीतकम्। यन्नाशे नियतं नाशो यस्मिंस्तिष्ठति तिष्ठति।।",
"spart2" : "निष्पद्यन्ते यतो भावा विविधा देहसंश्रयाः।"
},

{
 "chaptno" : "11",
 "slokano" : "039_2",
 "spart1" : "ओजः क्षीयेत कोपक्षुद्ध्यानशोकश्रमादिभिः।"
},

{
 "chaptno" : "11",
 "slokano" : "040",
 "spart1" : "बिभेति दुर्बलोऽभीक्ष्णं ध्यायति व्यथितेन्द्रियः।",
"spart2" : "दुःच्छायो दुर्मना रूक्षो भवेत्क्षामश्च तत्क्षये।।"
},

{
 "chaptno" : "11",
 "slokano" : "041",
 "spart1" : "जीवनीयौषधक्षीररसाद्यास्तत्र भेषजम्।",
"spart2" : "ओजोवृद्धौ हि देहस्य तुष्टिपुष्टिबलोदयः।।"
},

{
 "chaptno" : "11",
 "slokano" : "042",
 "spart1" : "यदन्नं द्वेष्टि यदपि प्रार्थयेताविरोधि तु।",
"spart2" : "तत्तत्त्यजन् समश्नंश्च तौ तौ वृद्धिक्षयौ जयेत्।।"
},

{
 "chaptno" : "11",
 "slokano" : "043",
"spart1": "कुर्वते हि रुचिं दोषा विपरीतसमानयोः।",
"spart2" : "वृद्धाः क्षीणाश्च भूयिष्ठं लक्षयन्त्यबुधास्तु न।।"
},

{
 "chaptno" : "11",
 "slokano" : "044",
 "spart1" : "यथाबलं यथास्वं च दोषा वृद्धा वितन्वते।",
"spart2" : "रूपाणि, जहति क्षीणाः, समाः स्वं कर्म कुर्वते।।"
},

{
 "chaptno" : "11",
 "slokano" : "045",
 "spart1" : "य एव देहस्य समा विवृद्ध्यै त एव दोषा विषमा वधाय।",
 "spart2" : "यस्मादतस्ते हितचर्ययैव क्षयाद्विवृद्धेरिव रक्षणीयाः।।"
},

{
 "chaptno" : "14",
 "slokano" : "000",
 "spart1" : "अथातो द्विविधोपक्रमणीयमध्यायं व्याख्यास्यामः",
 "spart2" : "इति ह स्माहुरात्रेयादयो महर्षयः।।"
},

{
 "chaptno" : "14",
 "slokano" : "001",
 "spart1" : "उपक्रम्यस्य हि द्वित्वाद्द्विधैवोपक्रमो मतः।",
 "spart2" : "एकः सन्तर्पणस्तत्र द्वितीयश्चापतर्पणः।।"
},

{
 "chaptno" : "14",
 "slokano" : "002",
 "spart1" : "बृंहणो लङ्घनश्चेति तत्पर्यायावुदाहृतौ।",
"spart2" : "बृंहणं यद्बृहत्वाय लङ्घनं लाघवाय यत्।।"
},

{
 "chaptno" : "14",
 "slokano" : "003",
 "spart1" : "देहस्य भवतः प्रायो भौमापमितरच्च ते।",
"spart2" : "स्नेहनं रूक्षणं कर्म स्वेदनं स्तम्भनं च यत्।।"
},

{
 "chaptno" : "14",
 "slokano" : "004",
 "spart1" : "भूतानां तदपि द्वैध्याद्द्वितयं नातिवर्तते।",
 "spart1" : "शोधनं शमनं चेति द्विधा तत्रापि लङ्घनम्"
},
{
 "chaptno" : "14",
 "slokano" : "005",
 "spart1" : "यदीरयेद्वहिर्दोषान् पञ्चधा शोधनं च तत्।",
"spart2" : "निरूहो वमनं कायशिरोरेकोऽस्रविस्रुतिः।।"
},

{
 "chaptno" : "14",
 "slokano" : "006",
 "spart1" : "न शोधयति यद्दोषान् समान्नोदीरयत्यपि।",
"spart2" : "समीकरोति विषमान् शमनं तच्च सप्तधा।।"
},

{
 "chaptno" : "14",
 "slokano" : "007",
 "spart1" : "पाचनं दीपनं क्षुत्तृड्व्यायामातपमारुताः।।",
 "spart1" : "बृंहणं शमनं त्वेव वायोः पित्तानिलस्य च।।"
},

{
 "chaptno" : "14",
 "slokano" : "008",
 "spart1" : "बृंहयेद्व्याधिभैषज्यमद्यस्त्रीशोककर्शितान्।",
"spart2" : "भाराध्वोरःक्षतक्षीणरूक्षदुर्बलवातलान्।।"
},

{
 "chaptno" : "14",
 "slokano" : "009",
 "spart1" : "गर्भिणीसूतिकाबालवृद्धान् ग्रीष्मेऽपरानपि।",
"spart2" : "मांसक्षीरसितासर्पिर्मधुरस्निग्धबस्तिभिः।।"
},

{
 "chaptno" : "14",
 "slokano" : "010",
 "spart1" : "स्वप्नशय्यासुखाभ्यङ्गस्नाननिर्वृतिहर्षणैः।",
"spart2" : "मेहामदोषातिस्निग्धज्वरोरुस्तम्भकुष्ठिनः।।"
},

{
 "chaptno" : "14",
 "slokano" : "011",
 "spart1" : "विसर्पविद्राधिप्लीहशिरःकण्ठाक्षिरोगिणः।",
"spart2" : "स्थूलांश्च लङ्घयेन्नित्यं शिशिरे त्वपरानपि।।"
},

{
 "chaptno" : "14",
 "slokano" : "012",
 "spart1" : "तत्र संशोधनैः स्थौल्यबलपित्तकफाधिकान्।",
"spart2" : "आमदोषज्वरच्छर्दिरतीसारहृदामयैः।।"
},

{
 "chaptno" : "14",
 "slokano" : "013",
 "spart1" : "विबन्धगौरवोद्गारहृल्लासादिभिरातुरान्।",
"spart2" : "मध्यस्थौल्यादिकान् प्रायः पूर्वं पाचनदीपनैः।।"
},

{
 "chaptno" : "14",
 "slokano" : "014",
 "spart1" : "एभिरेवामयैरार्तान् हीनस्थौल्यबलादिकान्।",
"spart2" : "क्षुत्तृष्णानिग्रहैर्दोषैस्त्वार्तान् मध्यबलैर्दृढान्।।"
},

{
 "chaptno" : "14",
 "slokano" : "015",
 "spart1" : "समीरणातपायासैः किमुताल्पबलैर्नरान्।",
"spart2" : "न बृंहयेल्लङ्घनीयान् बृंह्यांस्तु मृदु लङ्घयेत्।।"
},

{
 "chaptno" : "14",
 "slokano" : "016",
 "spart1" : "युक्त्या वा देशकालादिबलतस्तानुपाचरेत्।",
"spart2" : "बॄंहिते स्याद्बलं पुष्टिस्तत्साध्यामयसङ्क्षयः।।"
},

{
 "chaptno" : "14",
 "slokano" : "017",
 "spart1" : "विमलेन्द्रियता सर्गो मलानां लाघवं रुचिः।",
"spart2" : "क्षुत्तृट्सहोदयः शुद्धहृदयोद्गारकण्ठता।।"
},

{
 "chaptno" : "14",
 "slokano" : "018",
 "spart1" : "व्याधिमार्दवमुत्साहस्तन्द्रानाशश्च लङ्घिते।",
"spart2" : "अनपेक्षितमात्रादिसेविते कुरुतस्तु ते।।"
},

{
 "chaptno" : "14",
 "slokano" : "019",
 "spart1" : "अतिस्थौल्यातिकार्श्यादीन्, वक्ष्यन्ते च सौषधाः।",
"spart2" : "रूपं तैरेव च ज्ञेयमतिबृंहितलङ्घिते।।"
},

{
 "chaptno" : "14",
 "slokano" : "020",
 "spart1" : "अतिस्थौल्यापचीमेहज्वरोदरभगन्दरान्।",
 "spart2" : "काससन्न्यासकृच्छ्रामकुष्ठादीनतिदारुणान्।।"
},

{
 "chaptno" : "14",
 "slokano" : "021",
 "spart1" : "तत्र मेदोनिलश्लेष्मनाशनं सर्वमिष्यते।",
 "spart2" : "कुलत्थजूर्नश्यामाकयवमुद्गमधूदकम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "022",
 "spart1" : "मस्तुदण्डाहतारिष्टचिन्ताशोधनजागरम्।",
"spart2" : "मधुना त्रिफलां लिह्याद्गुडूचीमभयां धनम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "023",
 "spart1" : "रसाञ्जनस्य महतः पञ्चमूलस्य गुग्गुलोः।",
 "spart2" : "शिलाजतुप्रयोगश्च साग्निमन्थरसो हितः।।"
},

{
 "chaptno" : "14",
 "slokano" : "024",
 "spart1" : "विडड्गं नागरं क्षारः काललोहरजो मधु।",
 "spart1" : "यवामलकचूर्णं च योगोऽस्थौल्यदोषजित्।।"
},

{
 "chaptno" : "14",
 "slokano" : "025",
 "spart1" : "व्योषकट्वीवराशिग्रुविडङ्गातिविषास्थिराः।।",
"spart2" : "हिड्गुसौवर्चलाजाजीयवानीधान्यचित्रकाः।"
},

{
 "chaptno" : "14",
 "slokano" : "026",
 "spart1" : "निशे बृहत्यौ हपुषा पाठा मूलं च केम्बुकात्।",
 "spart1" : "एषां चूर्णं मधु घृतं तैलं च सदृशांशकम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "027",
 "spart1" : "सक्तुभिः षोडशगुणैर्युक्तं पीतं निहन्ति तत्।",
 "spart1" : "अतिस्थौल्यादिकान् सर्वान् रोगानान्यांश्च तद्द्विधान्।"
},

{
 "chaptno" : "14",
 "slokano" : "028",
 "spart1" : "हृद्रोगकामलाश्वित्रश्वासकासगलग्रहान्।",
"spart2" : "बुद्धिमेधास्मृतिकरं सन्नस्याग्नेश्च दीपनम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "029",
 "spart1" : "अतिकार्श्यं भ्रमः कासस्तृष्णाधिक्यमरोचकः।",
 "spart1" : "स्नेहाग्निनिद्रादृक्श्रोत्रशुक्रौजःक्षुत्स्वरक्षयः।।"
},

{
 "chaptno" : "14",
 "slokano" : "030",
"spart1" : "बस्तिहृन्मूर्धजङ्घोरुत्रिकपार्श्वरुजा ज्वरः।",
"spart2" : "प्रलापोर्ध्वानिलग्लानिच्छर्दिपर्वास्थिभेदनम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "031",
 "spart1" : " वर्चोमूत्रग्रहाद्याश्च जायन्तेऽतिविलङ्घनात्।",
"spart2" : "कार्श्यमेव वरं स्थौल्यात् न हि स्थूलस्य भेषजम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "032",
 "spart1" : "बृंहणं लङ्घनं वाऽलमतिमेदोग्निवातजित्।",
"spart2" : "मधुरस्निग्धसौहित्यैर्यत्सौख्येन च नश्यति।।"
},

{
 "chaptno" : "14",
 "slokano" : "033",
 "spart1" : "क्रशिमा स्थविमाऽत्यन्तविपरीतनिषेवणैः|",
"spart2" : "योजयेद्धृंहणं तत्र सर्वं पानान्नभेषजम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "034",
 "spart1" : "अचिन्तया हर्षणेन ध्रुवं सन्तर्पणेन च।",
"spart2" : "स्वप्नप्रसङ्गाच्च कृशो वराह इव पुष्यति।।"
},

{
 "chaptno" : "14",
 "slokano" : "035",
 "spart1" : "न हि मांससमं किञ्चिदन्यद्देहबृहत्त्वकृत्।",
"spart2" : "मांसादमांसं मांसेन सम्भृतत्वाद्विशेषतः।।"
},

{
 "chaptno" : "14",
 "slokano" : "036",
"spart1": " गुरु चातर्पणं स्थूले विपरीतं हितं कृशे।",
"spart2" : "यवगोधूममुभयोस्तद्योग्याहितकल्पनम्।।"
},

{
 "chaptno" : "14",
 "slokano" : "037",
 "spart1" : "दोषगत्याऽतिरिच्यन्ते ग्राहिभेद्यादिभेदतः।",
"spart2" : "उपक्रमा न ते द्वित्वाद्भिन्ना अपि गदा इव।।"
}

]
